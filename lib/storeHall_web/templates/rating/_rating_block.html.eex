
<rating_block>
  <div class="review_score_badge" data-content="<%= @rating["score"] %>">
    <%= @rating["score"] %>
  </div>
  <div class="review_score_content">
    <div class="review_score_title">
    </div>
    <div class="review_score_count">
      <%= @rating["count"] %> <%= Gettext.gettext("ratings") %>
    </div>
  </div>
</rating_block>
